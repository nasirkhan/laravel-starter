{
    "name": "nasirkhan/laravel-starter",
    "description": "CMS like a modular Laravel Starter project.",
    "keywords": ["framework", "laravel", "cms", "starter", "admin"],
    "license": "GPL-3.0-or-later",
    "type": "project",
    "version": "v5.0.0",
    "require": {
        "php": "^7.2",
        "alexusmai/laravel-file-manager": "^2.4",
        "arcanedev/log-viewer": "^5.2",
        "fideloper/proxy": "^4.0",
        "hashids/hashids": "^4.0",
        "hieu-le/active": "^4.0",
        "laracasts/flash": "^3.0",
        "laravel/framework": "^6.2",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/socialite": "^4.3",
        "laravel/tinker": "^2.0",
        "lavary/laravel-menu": "^1.7",
        "mews/purifier": "^3.1",
        "nwidart/laravel-modules": "^6.2",
        "predis/predis": "^1.1",
        "spatie/laravel-activitylog": "^3.9",
        "spatie/laravel-backup": "^6.7",
        "spatie/laravel-feed": "^2.6",
        "spatie/laravel-html": "^2.24",
        "spatie/laravel-medialibrary": "^7.18",
        "spatie/laravel-permission": "^3.8",
        "yajra/laravel-datatables": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "laravel/ui": "^1.2",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories",
            "app/Models"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "clear-all": [
            "composer dumpautoload -o",
            "@php artisan clear-compiled",
            "@php artisan cache:clear",
            "@php artisan route:clear",
            "@php artisan view:clear",
            "@php artisan config:clear",
            "@php artisan cache:forget spatie.permission.cache"
        ]
    }
}
