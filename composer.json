{
    "name": "nasirkhan/laravel-starter",
    "description": "CMS like a modular Laravel Starter project.",
    "keywords": ["framework", "laravel", "cms", "starter", "admin"],
    "license": "GPL-3.0-or-later",
    "type": "project",
    "version": "v2.5.0",
    "require": {
        "php": "^7.1.3",
        "arcanedev/log-viewer": "^4.7",
        "fideloper/proxy": "^4.1",
        "hashids/hashids": "^4.0",
        "hieu-le/active": "^3.5",
        "laracasts/flash": "^3.0",
        "laravel/framework": "5.8.*",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/socialite": "^4.1",
        "laravel/tinker": "^1.0",
        "lavary/laravel-menu": "^1.7",
        "mews/purifier": "^2.1",
        "nwidart/laravel-modules": "^5.0",
        "predis/predis": "^1.1",
        "spatie/laravel-activitylog": "^3.6",
        "spatie/laravel-backup": "^6.1",
        "spatie/laravel-html": "^2.21",
        "spatie/laravel-medialibrary": "^7.6",
        "spatie/laravel-permission": "^2.37",
        "unisharp/laravel-filemanager": "^1.9",
        "yajra/laravel-datatables": "^1.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "filp/whoops": "^2.3",
        "fzaninotto/faker": "^1.8",
        "mockery/mockery": "^1.2",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^7.3"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories",
            "app/Models"
        ],
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "clear-all": [
            "composer dumpautoload -o",
            "@php artisan clear-compiled",
            "@php artisan cache:clear",
            "@php artisan route:clear",
            "@php artisan view:clear",
            "@php artisan config:clear",
            "@php artisan cache:forget spatie.permission.cache"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
